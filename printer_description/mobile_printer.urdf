    <origin rpy="0 0 0" xyz="0.0 0 .2"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="0.0" upper="1" velocity="0.5"/>
  </joint>

  <joint name="elbow_joint" type="continuous">
    <parent link="prism_link"/>
    <child link="elbow_link"/>
